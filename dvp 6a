import matplotlib.pyplot as plt
import numpy as np
# Generate sample data
x = np.linspace(0, 10, 100)
y = 2 * x + 3
# Simple line plot
plt.plot(x, y, label='y = 2x + 3', color='blue', 
 linestyle='-', linewidth=2)
# Adding labels and title
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.title('Simple Line Plot')
# Adding legend
plt.legend()
# Show the plot
plt.show()
